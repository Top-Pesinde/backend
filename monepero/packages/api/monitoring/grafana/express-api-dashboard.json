{
    "id": null,
    "title": "Express API Monitoring",
    "tags": [
        "express",
        "nodejs",
        "api"
    ],
    "style": "dark",
    "timezone": "browser",
    "refresh": "30s",
    "time": {
        "from": "now-1h",
        "to": "now"
    },
    "timepicker": {},
    "templating": {
        "list": []
    },
    "panels": [
        {
            "id": 1,
            "title": "HTTP Requests per Second",
            "type": "stat",
            "targets": [
                {
                    "expr": "rate(http_requests_total[5m])",
                    "legendFormat": "{{method}} {{route}}",
                    "refId": "A"
                }
            ],
            "gridPos": {
                "h": 8,
                "w": 12,
                "x": 0,
                "y": 0
            },
            "fieldConfig": {
                "defaults": {
                    "unit": "reqps",
                    "color": {
                        "mode": "palette-classic"
                    }
                }
            }
        },
        {
            "id": 2,
            "title": "HTTP Response Status Codes",
            "type": "piechart",
            "targets": [
                {
                    "expr": "sum by (status_code) (http_requests_total)",
                    "legendFormat": "{{status_code}}",
                    "refId": "A"
                }
            ],
            "gridPos": {
                "h": 8,
                "w": 12,
                "x": 12,
                "y": 0
            }
        },
        {
            "id": 3,
            "title": "HTTP Request Duration",
            "type": "timeseries",
            "targets": [
                {
                    "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket[5m]))",
                    "legendFormat": "95th percentile",
                    "refId": "A"
                },
                {
                    "expr": "histogram_quantile(0.50, rate(http_request_duration_seconds_bucket[5m]))",
                    "legendFormat": "50th percentile",
                    "refId": "B"
                }
            ],
            "gridPos": {
                "h": 8,
                "w": 24,
                "x": 0,
                "y": 8
            },
            "fieldConfig": {
                "defaults": {
                    "unit": "s"
                }
            }
        },
        {
            "id": 4,
            "title": "Authentication Metrics",
            "type": "timeseries",
            "targets": [
                {
                    "expr": "rate(auth_success_total[5m])",
                    "legendFormat": "Success - {{type}}",
                    "refId": "A"
                },
                {
                    "expr": "rate(auth_failures_total[5m])",
                    "legendFormat": "Failures - {{type}}",
                    "refId": "B"
                }
            ],
            "gridPos": {
                "h": 8,
                "w": 12,
                "x": 0,
                "y": 16
            }
        },
        {
            "id": 5,
            "title": "User Registrations Total",
            "type": "stat",
            "targets": [
                {
                    "expr": "user_registrations_total",
                    "legendFormat": "{{role}}",
                    "refId": "A"
                }
            ],
            "gridPos": {
                "h": 8,
                "w": 12,
                "x": 12,
                "y": 16
            }
        },
        {
            "id": 6,
            "title": "System Resources",
            "type": "timeseries",
            "targets": [
                {
                    "expr": "process_resident_memory_bytes / 1024 / 1024",
                    "legendFormat": "Memory Usage (MB)",
                    "refId": "A"
                },
                {
                    "expr": "rate(process_cpu_seconds_total[5m]) * 100",
                    "legendFormat": "CPU Usage (%)",
                    "refId": "B"
                }
            ],
            "gridPos": {
                "h": 8,
                "w": 24,
                "x": 0,
                "y": 24
            },
            "fieldConfig": {
                "defaults": {
                    "unit": "short"
                }
            }
        }
    ],
    "schemaVersion": 39,
    "version": 1,
    "links": []
}